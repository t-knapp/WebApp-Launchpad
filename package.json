{
  "name": "gma-web-simulator",
  "version": "1.0.0",
  "description": "GMA Web Simulator",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.start.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "test": "jest --coverage --silent",
    "test:watch": "jest --watchAll",
    "analyze": "webpack --config webpack.prod.js --profile --json > stats.json"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|dll)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "@builder/(.*)$": "<rootDir>/src/refactored/builders/$1",
      "@component/(.*)$": "<rootDir>/src/refactored/components/$1",
      "@exception/(.*)$": "<rootDir>/src/refactored/exceptions/$1",
      "@helper/(.*)$": "<rootDir>/src/refactored/helpers/$1",
      "@model/(.*)$": "<rootDir>/src/refactored/models/$1",
      "@modelProvider/(.*)$": "<rootDir>/src/refactored/models/modelProviders/$1",
      "@provider/(.*)$": "<rootDir>/src/refactored/providers/$1",
      "@repository/(.*)$": "<rootDir>/src/refactored/repositories/$1",
      "@service/(.*)$": "<rootDir>/src/refactored/services/$1",
      "@viewModel/(.*)$": "<rootDir>/src/refactored/viewModels/$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/less/"
    ],
    "testMatch": [
      "**/__test__/*.test.js?(x)",
      "**/__test__/*.test.ts?(x)"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/testFrameworkSetup.js",
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/@typings/**",
      "!src/**/I[A-Z]*.js",
      "!src/**/I[A-Z]*.ts"
    ],
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript"
      }
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cordova": "0.0.34",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.9.0",
    "@types/lodash": "^4.14.138",
    "@types/pouchdb": "^6.4.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-test-renderer": "^16.9.2",
    "@types/semver": "^7.1.0",
    "autobind-decorator": "^2.4.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-nameof-js": "0.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "colors": "^1.3.3",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.5.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^1.1.11",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "i18n-validation": "git+http://bitbucket:7990/scm/rw/i18n-validation.git#semver:^1.0.0",
    "ifdef-loader": "^2.1.4",
    "jest": "^23.6.0",
    "jest-canvas-mock": "^2.1.0",
    "jest-enzyme": "^7.0.1",
    "jest-fetch-mock": "^2.1.2",
    "json-loader": "^0.5.7",
